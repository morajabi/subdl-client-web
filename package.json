    "babel-preset-stage-1": "^6.24.1",
    "css-color-function": "^1.3.0",
    "grade-js": "^1.0.10",
    "graphql-tag": "^2.4.2",
    "lodash": "^4.17.4",
    "normalize.css": "^6.0.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "^15.5.4",
